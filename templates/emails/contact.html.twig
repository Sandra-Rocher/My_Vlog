{# Template of contact email :  #}

<p>Une nouvelle demande de contact a été reçue</p>
<ul>
    <li>Nom : {{ data.lastName }}</li>
    <li>Prénom : {{ data.firstName }}</li>
    <li>Ville : {{ data.city }}</li>
    <li>Email : {{ data.email }}</li>
    <li>Sujet : {{ data.subject }}</li>
</ul>

<p><strong>Message :</strong></p>
<p>{{ data.message | nl2br }}</p>

